{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:app:allow-app-hide",
    "core:app:allow-app-show"
  ]
}
